<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>launched.proj-backup</string>
    <key>ProgramArguments</key>
    <array>
	<string>sh</string>
	<string>-c</string>
      	<string>$HOME/github/project-backup/proj-backup.sh</string>
    </array>
    <key>StandardErrorPath</key>
    <string>$HOME/backup_launchd.err</string>
    <key>StandardOutPath</key>
    <string>$HOME/backup_launchd.out</string>
    <key>StartCalendarInterval</key>
    <array>
      <dict>
        <key>Hour</key>
        <integer>14</integer>
        <key>Minute</key>
        <integer>45</integer>
      </dict>
    </array>
  </dict>
</plist>
